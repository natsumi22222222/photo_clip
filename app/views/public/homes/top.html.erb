<%= link_to "マップから写真を探す" %>

<div id='map'></div>

<style>
    #map {
        height: 500px;
        width: 70%;
    }
</style>

<script>
    let map

    const display = document.getElementById('display')


    function initMap() {
        geocoder = new google.maps.Geocoder()

        map = new google.maps.Map(document.getElementById('map'), {
            center: {
                lat: 35.001637,
                lng: 135.767667
            },
            zoom: 15,
        });
     }
</script>

<script src="https://maps.googleapis.com/maps/api/js?key=<%= Rails.application.credentials.Google_API_KEY %>&callback=initMap" async defer >
</script>