<div class="container text-center">
  <div class= "row">
    <div class="col-12">
      <img src= 'assets/marker.jpeg' alt= "" class= "marker-image">
      <h3>自然の写真一覧</h3>
    </div>
  </div>
  <div class="row">
    <div class="col-12">
      <% @posts.each do |post| %>
        <%= image_tag post.get_image, class: "post-image" %>
      <% end %>
    </div>
  </div>
</div>
