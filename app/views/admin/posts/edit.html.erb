<h2>管理者投稿編集</h2>
<%= form_with model: @post, url: edit_admin_post_path(@post.id), method: :patch do |f| %>
  <%= f.file_field :image, placeholder: "投稿画像", accept: "image/*"  %>
  <%= f.label :title, "タイトル" %>
  <%= f.text_field :title %>
  <%= f.label :body, "詳細文" %>
  <%= f.text_area :body %>
  <%= f.submit "変更を保存" %>
<% end %>